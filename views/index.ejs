<!-- Navbar -->

<body data-home-page="index.ejs" data-home-page-title="Index" class="u-body u-stick-footer u-xl-mode">
  <header class="u-clearfix u-grey-5 u-header u-header" id="sec-e603">
    <div class="u-clearfix u-sheet u-sheet-1">
      <div class="u-align-left u-container-style u-group u-shape-rectangle u-group-1">
        <div class="u-container-layout u-container-layout-1">
          <img class="u-align-center u-image u-image-default u-preserve-proportions u-image-1"
            src="assets/images/logo.png" alt="" data-image-width="91" data-image-height="89" width="150" height="70">
        </div>
      </div>
      <p class="u-align-center u-text u-text-custom-color-1 u-text-default u-text-1">FarmLink</p>
      <a class="u-shopping-cart u-shopping-cart-1"></a>
      <nav class="u-menu u-menu-dropdown u-offcanvas u-menu-1">
      </nav>
      <h3 class="u-align-center u-headline u-text u-text-custom-color-1 u-text-2">
        <a>FarmLink</a>
      </h3>
    </div>
  </header>

  <!-- Website Body -->
  <section class="u-clearfix u-section-1" id="sec-410e">
    <div class="u-clearfix u-sheet u-sheet-1">
      <div class="u-clearfix u-expanded-width u-layout-wrap u-layout-wrap-1">
        <div class="u-layout">
          <div class="u-layout-row">
            <div class="u-container-style u-layout-cell u-size-30 u-layout-cell-1">
              <div class="u-container-layout u-container-layout-1">
                <h2 class="u-align-center u-text u-text-default u-text-1">Marketplace Online </h2>
                <img class="u-image u-image-round u-radius-10 u-image-1" src="assets/images/indexImage.png"
                  alt="indexImage" data-image-width="533" data-image-height="335">
                <p class="u-align-left u-text u-text-2">Aqui puderá encontrar produtos frescos e da época,<br>comprados
                  diretamente ao produtor e entregues ao seu domicilio no dia da sua colheita!
                </p>
              </div>
            </div>
            <div class="u-align-left u-container-style u-layout-cell u-size-30 u-layout-cell-2">
              <div class="u-container-layout u-container-layout-2">
                <h2 class="u-align-center u-text u-text-default u-text-3">Entrar </h2>

                <!-- Login Form -->
                <div class="u-form u-form-1">
                  <form action="/login" method="POST" style="padding: 15px;">
                    <div class="u-form-group u-form-name u-label-none" style="margin-bottom: 4%;">
                      <input type="email" placeholder="Introduza o seu email" id="email" name="email"
                        class="u-border-1 u-border-grey-30 u-input u-input-rectangle" required>
                    </div>
                    <div class="u-form-email u-form-group u-label-none">
                      <input type="password" placeholder="Introduza a sua password" id="pass" name="password"
                        class="u-border-1 u-border-grey-30 u-input u-input-rectangle" required>
                    </div>
                    <div class="u-align-center u-form-group u-form-submit">
                      <button type="submit"
                        class="u-btn u-btn-round u-btn-submit u-button-style u-radius-6 u-btn-1">Autenticar</button>
                    </div>
                  </form>
                </div>
                <!-- Register Redirect -->
                <div class="u-border-3 u-border-grey-dark-1 u-line u-line-horizontal u-line-1"
                  style="margin-top: 4.5%;"></div>
                <p class="u-align-center u-text u-text-default u-text-4">ou</p>
                <div class="u-border-3 u-border-grey-dark-1 u-line u-line-horizontal u-line-2"></div>
                <a href="/signUp"
                  class="u-btn u-btn-round u-button-style u-hover-palette-1-dark-1 u-palette-1-base u-radius-6 u-btn-2">Registar</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Footer -->
  <footer class="u-align-center u-clearfix u-footer u-grey-10 u-footer" id="sec-984e">
    <div class="u-clearfix u-sheet u-sheet-1">
